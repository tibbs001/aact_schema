    s.number_of_arms,
    s.enrollment,
    regexp_replace(s.baseline_population, '[

 ]+'::text, ' '::text, 'g'::text) AS baseline_population,
    regexp_replace((s.why_stopped)::text, '[

 ]+'::text, ' '::text, 'g'::text) AS why_stopped,
    regexp_replace((s.limitations_and_caveats)::text, '[

 ]+'::text, ' '::text, 'g'::text) AS limitations_and_caveats,
    cv.number_of_facilities,
    cv.actual_duration,
    cv.were_results_reported,
