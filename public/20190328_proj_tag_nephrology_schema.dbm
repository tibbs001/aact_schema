    d.allocation,
    d.intervention_model,
    d.primary_purpose,
    d.time_perspective,
    d.masking
   FROM studies s,
    calculated_values cv,
    designs d
  WHERE (((s.nct_id)::text = (cv.nct_id)::text) AND ((s.nct_id)::text = (d.nct_id)::text) AND ((s.nct_id)::text IN ( SELECT DISTINCT s_1.nct_id
           FROM studies s_1,
            browse_conditions bc
          WHERE (((s_1.study_type)::text = 'Interventional'::text) AND (s_1.study_first_posted_date >= '2007-10-01'::date) AND ((s_1.nct_id)::text = (bc.nct_id)::text) AND ((bc.downcase_mesh_term)::text IN ( SELECT tagged_terms.term
                   FROM proj_tag_nephrology.tagged_terms)))
        UNION
         SELECT DISTINCT s_1.nct_id
           FROM studies s_1,
            keywords k
          WHERE (((s_1.study_type)::text = 'Interventional'::text) AND (s_1.study_first_posted_date >= '2007-10-01'::date) AND ((s_1.nct_id)::text = (k.nct_id)::text) AND ((k.downcase_name)::text IN ( SELECT tagged_terms.term
                   FROM proj_tag_nephrology.tagged_terms)))
        UNION
         SELECT DISTINCT s_1.nct_id
           FROM studies s_1,
            conditions c
          WHERE (((s_1.study_type)::text = 'Interventional'::text) AND (s_1.study_first_posted_date >= '2007-10-01'::date) AND ((s_1.nct_id)::text = (c.nct_id)::text) AND ((c.downcase_name)::text IN ( SELECT tagged_terms.term
                   FROM proj_tag_nephrology.tagged_terms))))));]]></expression>
